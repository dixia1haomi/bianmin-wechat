/**app.wxss**/

page {
  height: 100%;
  background-color: #efefef;
  /* background-color: tomato; */
  font-size: 32rpx;
  letter-spacing: 1rpx;
  color: #3e3e3e;
}

/* 登陆弹窗 */

.loginTanChuang-mask {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  background-color: rgba(0, 0, 0, 0.5);
  display: flex;
  justify-content: center;
}

.loginTanChuang {
  width: 500rpx;
  height: 300rpx;
  border-radius: 4rpx;
  background-color: #fff;
  margin-top: 300rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}

.loginTanChuang-title {
  text-align: center;
  font-size: 36rpx;
  font-weight: 700;
}

.loginTanChuang-XuanXiang{
display: flex;
justify-content: space-around;
}

