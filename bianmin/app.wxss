/* 带箭头的单行列表 */

.danhang-list {
  padding: 20rpx;
  background-color: #fff;
  /* margin-top: 10rpx; */
  border-top: 1px solid #e5e5e5;
  border-bottom: 1px solid #e5e5e5;
  font-size: 34rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

/* flex */

.flex {
  display: flex;
}

/* 字体大小 */

.font24 {
  font-size: 24rpx;
}

.font28 {
  font-size: 28rpx;
}

.font32 {
  font-size: 32rpx;
}

.font36 {
  font-size: 36rpx;
}

/* view点击态 */

.hover {
  /* background-color: #e5e5e5; */
  color: #8a8a8a;
}

/* 右箭头 */

.youjiantou7 {
  width: 7px;
  height: 7px;
  border-top: 3rpx solid #8a8a8a;
  border-right: 3rpx solid #8a8a8a;
  transform: rotate(45deg);
}
