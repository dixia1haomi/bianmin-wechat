@font-face {
  font-family: "iconfont";
  src: url('//at.alicdn.com/t/font_628354_ufzcv5zf8hncdi.eot?t=1524050057553'); /* IE9*/
  src: url('//at.alicdn.com/t/font_628354_ufzcv5zf8hncdi.eot?t=1524050057553#iefix') format('embedded-opentype'),
         /* IE6-IE8 */ url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
         url('//at.alicdn.com/t/font_628354_ufzcv5zf8hncdi.ttf?t=1524050057553') format('truetype'),
         /* chrome,
         firefox,
         opera,
         Safari,
         Android,
         iOS 4.2+*/ url('//at.alicdn.com/t/font_628354_ufzcv5zf8hncdi.svg?t=1524050057553#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-phone:before {
  content: "\e61a";
}

.icon-dizhi:before {
  content: "\e641";
}

.icon-yueduliang:before {
  content: "\e671";
}

.icon-zhankai:before {
  content: "\e613";
}

.icon-shangjia-dianhua:before {
  content: "\e63d";
}

